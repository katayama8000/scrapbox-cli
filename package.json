{
  "name": "scrapbox-cli",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "daily": "ts-node -r dotenv/config -r tsconfig-paths/register src/infrastructure/cli/blog-cli.ts daily",
    "weekly": "ts-node -r dotenv/config -r tsconfig-paths/register src/infrastructure/cli/blog-cli.ts weekly",
    "sleepLog": "ts-node -r dotenv/config -r tsconfig-paths/register src/infrastructure/cli/sleepLog-cli.ts",
    "wake": "ts-node -r dotenv/config -r tsconfig-paths/register src/infrastructure/cli/wake-cli.ts",
    "typeCheck": "tsc --noEmit"
  },
  "dependencies": {
    "@katayama8000/cosense-client": "npm:@jsr/katayama8000__cosense-client",
    "dayjs": "1.11.18",
    "puppeteer": "24.22.0"
  },
  "devDependencies": {
    "dotenv": "17.2.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.2"
  }
}